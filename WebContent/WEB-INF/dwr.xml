		<convert converter="bean" match="br.com.dfdevforge.sisfin.bean.BtpTemplateRegra" />
		<convert converter="bean" match="br.com.dfdevforge.sisfin.bean.BtpFormaPagamento" />
		<convert converter="bean" match="br.com.dfdevforge.sisfin.estabelecimento.bean.BtpEstabelecimento" />

		<convert converter="bean" match="br.com.dfdevforge.sisfin.behavior.TransferObject" />
