			<param name="teste" value="xana" />
		</filter>

		<create creator="new" javascript="DwrResumo">
			<param name="class" value="br.com.dfdevforge.sisfin.dwr.DwrResumo" />
		</create>
		<create creator="new" javascript="DwrMovimentoNew">
			<param name="class" value="br.com.dfdevforge.sisfin.dwr.DwrMovimentoNew" />
		</create>

		<create creator="new" javascript="DwrMovimento">
			<param name="class" value="br.com.dfdevforge.sisfin.dwr.DwrMovimento" />
		</create>

		<create creator="new" javascript="DwrObjetivo">
			<param name="class" value="br.com.dfdevforge.sisfin.dwr.DwrObjetivo" />
		</create>

		<create creator="new" javascript="DwrTemplate">
			<param name="class" value="br.com.dfdevforge.sisfin.dwr.DwrTemplate" />
