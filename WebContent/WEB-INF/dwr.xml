		<create creator="new" javascript="DwrTemplateRegra">
			<param name="class" value="br.com.dfdevforge.sisfin.dwr.DwrTemplateRegra" />
		</create>

		<create creator="new" javascript="DwrExtrato">
			<param name="class" value="br.com.dfdevforge.sisfin.dwr.DwrExtrato" />
		</create>

		<create creator="new" javascript="DwrItemExtrato">
			<param name="class" value="br.com.dfdevforge.sisfin.dwr.DwrItemExtrato" />
		</create>

		<convert converter="bean" match="br.com.dfdevforge.sisfin.dwr.DwrReturn" />

		<convert converter="bean" match="br.com.dfdevforge.sisfin.bean.BtpBanco" />
		<convert converter="bean" match="br.com.dfdevforge.sisfin.bean.BtpConta" />
		<convert converter="bean" match="br.com.dfdevforge.sisfin.bean.BtpRegra" />
		<convert converter="bean" match="br.com.dfdevforge.sisfin.bean.BtpExtrato" />
